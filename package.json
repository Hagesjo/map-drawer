{
    "name": "mapbox",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "npm run server:build && npm run client:build",
        "build:server": "cd server && rollup -c",
        "build:client": "cd client && snowpack build",
        "dev": "stmux -w always -e \"\\[ERROR\\]\" -m beep,system -M -- [ \"npm run dev:server\" : \"npm run dev:client\" ]",
        "dev:server": "cd server && nodemon src/index.ts",
        "dev:client": "cd client && snowpack dev"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@mapbox/mapbox-gl-draw": "^1.3.0",
        "@rollup/plugin-typescript": "^8.3.0",
        "@snowpack/plugin-dotenv": "^2.2.0",
        "@snowpack/plugin-postcss": "^1.4.3",
        "@snowpack/plugin-typescript": "^1.2.1",
        "@types/express": "^4.17.13",
        "@types/mapbox__mapbox-gl-draw": "^1.2.3",
        "@types/mapbox-gl": "^2.6.0",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "cross-env": "^7.0.3",
        "http2-proxy": "^5.0.53",
        "mapbox-gl": "^2.6.1",
        "nodemon": "^2.0.15",
        "normalize.css": "^8.0.1",
        "postcss": "^8.4.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rollup": "^2.61.1",
        "rollup-plugin-cleaner": "^1.0.0",
        "snowpack": "^3.8.8",
        "socket.io-client": "^4.4.0",
        "source-map-support": "^0.5.21",
        "stmux": "^1.8.3",
        "tailwindcss": "^3.0.0",
        "ts-node": "^10.4.0"
    },
    "dependencies": {
        "express": "^4.17.1",
        "socket.io": "^4.4.0"
    }
}
